{
    "reporter": [
        {
            "name": "spec"
        },
        {
			"name": "allure-expanded"
		}
    ],
    "videoPath": "Videos/",
	"videoOptions": {
		"singleFile": false,
		"failedOnly": true,
		"pathPattern": "${DATE}_${TIME}/${TEST}/${USERAGENT}/${FILE_INDEX}.mp4"
    },
    "screenshots": {
		"path": "Screenshots/",
		"takeOnFails": true,
		"pathPattern": "${DATE}_${TIME}/${TEST}/${USERAGENT}/${QUARANTINE_ATTEMPT}.png"
    }
}